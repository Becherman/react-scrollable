{
  "name": "react-scrollx",
  "version": "0.1.2",
  "description": "Simple customizing scroll react component",
  "keywords": "react, scroll, scrolling, scrolls, custom-scroll",
  "main": "dist/react-scrollx.js",
  "homepage": "https://github.com/Becherman/react-scrollx",
  "scripts": {
    "start": "webpack-dev-server --env=development",
    "dev": "webpack --env=development || true",
    "build": "webpack --env=production -p --colors --progress",
    "lint": "node_modules/.bin/eslint",
    "page-build": "rm -f index.html && webpack --config webpack.page.config.js --env=production -p --colors --progress",
    "page-dev": "webpack-dev-server --env=development --config webpack.page.config.js",
    "flow": "node_modules/.bin/flow"
  },
  "author": "Serj Bulavyk <serg.bulavyk@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": "^15.6.x",
    "react-dom": "^15.6.x",
    "prop-types": "^15.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.0.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-loader": "^1.8.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "flow-bin": "^0.65.0",
    "flow-typed": "^2.3.0",
    "html-webpack-plugin": "^2.30.1",
    "prettier": "^1.11.1",
    "prop-types": "^15.6.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "style-loader": "^0.18.2",
    "styled-components": "^3.1.6",
    "uglifyjs-webpack-plugin": "^1.1.0",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.5.0"
  },
  "dependencies": {
    "react-router": "^4.2.0"
  }
